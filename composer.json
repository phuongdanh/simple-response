{
    "name": "phuongdanh/simple-response",
    "description": "This package is used to wrap your response data. It supports for laravel as well",
    "keywords": [
        "api",
        "json",
        "xml"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Danh tp",
            "email": "tuphuongdanh1996@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {"SimpleResponse\\": "src/"}
    },
    "require": {
        "illuminate/support": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SimpleResponse\\ServiceProvider"
            ]
        }
    }
}
